{
  "name": "ubio_tech_test",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "out/bin/serve.js",
  "exports": {
    ".": "./out/main/index.js",
    "./modules/mongodb": "./out/modules/mongodb.js"
  },
  "bin": {
    "generate-openapi": "out/bin/generate-openapi.js",
    "generate-docs": "out/bin/generate-docs.js",
    "generate-env": "out/bin/generate-env.js"
  },
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "start": "node out/bin/serve",
    "dev": "npm run clean && tsc -w",
    "lint": "eslint --ext=.js,.ts --cache .",
    "clean": "rm -rf out",
    "compile": "npm run clean && tsc",
    "test": "NODE_ENV=test mocha",
    "docs": "npm run docs:openapi && npm run docs:api",
    "docs:openapi": "generate-openapi > openapi.json",
    "docs:api": "generate-docs > API.md"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/koa": "^2.15.0",
    "@types/mongodb": "^4.0.7",
    "@ubio/framework": "^15.11.1",
    "dotenv": "^16.4.5",
    "koa": "^2.15.3",
    "mesh-ioc": "^3.3.1",
    "mongodb": "^6.6.2"
  },
  "devDependencies": {
    "@nodescript/eslint-config": "^1.2.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.12",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.57.0",
    "mocha": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.3",
    "typescript": "^5.4.5"
  }
}
